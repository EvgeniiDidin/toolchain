/* ARC AXS102 SDP - HS34 */

MEMORY
{
    MBRAM : ORIGIN = 0x00000000, LENGTH = 256K
    ICCM  : ORIGIN = 0x10000000, LENGTH = 64K
    SRAM  : ORIGIN = 0x20000000, LENGTH = 256K
    DRAM  : ORIGIN = 0x80000000, LENGTH = 1024M
    DCCM  : ORIGIN = 0xC0000000, LENGTH = 64K
}

REGION_ALIAS("startup", MBRAM)
REGION_ALIAS("text", ICCM)
REGION_ALIAS("data", DCCM)
REGION_ALIAS("sdata", DCCM)

PROVIDE (__stack_top = (0xBFFFFFFF & -4) );
PROVIDE (__end_heap = (0xBFFFFFFF) );
